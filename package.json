{
  "name": "api_agencia_modelo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint --fix",
      "npm run prettier",
      "npm run test:staged --bail"
    ]
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "start:all": "npm run start:docker && npm run prisma:up && npm run dev",
    "start:docker": "docker-compose up -d && sleep 5 && echo 'ContÃªineres Docker iniciados com sucesso!'",
    "prisma:up": "npx prisma migrate status --skip-generate || npx prisma migrate dev --name init",
    "dev": "tsnd --exit-child --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --write src/**/*.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watch",
    "test:staged": "npm run test --findRelatedTests",
    "test:push": "npm run test --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "git-commit-msg-linter": "^4.1.3",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.15.0",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  }
}
